function complete_goal(goal_id){
	
	if (goal_id.sprite_index = Goal_blue) {
		
		goal_id.sprite_index = Completed_blue
	
	}
	if (goal_id.sprite_index = Goal_green) {
		
		goal_id.sprite_index = Completed_green
	
	}

	if (goal_id.sprite_index = Goal_red) {
		
		goal_id.sprite_index = Completed_red
	
	}
}